<% provide(:title, 'All users') %>
<h1>All users</h1>
<ul class="users">
  <li>
    <% @users.each do |user| %>
      <% if !current_user?(user) %>
        <li><%= link_to user.name, user %>
        <%= user.email %>
        <%= link_to "edit", edit_admin_user_path(user) %>
        <%= link_to "delete", admin_user_path(user), method: :delete,
                              data: { confirm: "You sure?" } %>
        </li>
      <% end %>
    <% end %>
  </li>
</ul>
